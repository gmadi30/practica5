package estructurasdedatos;

import java.util.Scanner;


public class Ejercicio1 {

	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		
		
		int numeroUsuario;
		int minimo;
		int maximo;
		
		System.out.println("Introduzca un minimo: ");
		minimo = entrada.nextInt();
		System.out.println("Introduzca un maximo: ");
		maximo = entrada.nextInt();
		
		do {
			
			System.out.println("Introduzca un valor: ");
			numeroUsuario = entrada.nextInt();
			if (numeroUsuario <= minimo || numeroUsuario >= maximo) {
				System.out.println("Valor fuera de rango, introduzca un nuevo valor.");
			} else {
				System.out.println("Valor dentro del rango");
			}
			
		} while(numeroUsuario <= minimo || numeroUsuario >= maximo);

	}

}
